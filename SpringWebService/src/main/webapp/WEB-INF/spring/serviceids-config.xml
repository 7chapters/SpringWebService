<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:sws="http://www.springframework.org/schema/web-services"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">

	<util:map id="serviceIds" map-class="java.util.HashMap"
		key-type="java.lang.String" value-type="java.lang.String">
		<entry key="${WS.SERVICE.USERNAME.PWOWS}" value="${WS.SERVICE.PASSWORD.PWOWS}" />
		<entry key="${WS.SERVICE.USERNAME.TSCIR}" value="${WS.SERVICE.PASSWORD.TSCIR}" />
	</util:map>

	<alias name="serviceIds" alias="users" />

</beans>
